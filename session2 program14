class Pair<T, U> {
    private T first;
    private U second;

    // Constructor
    public Pair(T first, U second) {
        this.first = first;
        this.second = second;
    }

    // Getter and setter for first element
    public T getFirst() {
        return first;
    }

    public void setFirst(T first) {
        this.first = first;
    }

    // Getter and setter for second element
    public U getSecond() {
        return second;
    }

    public void setSecond(U second) {
        this.second = second;
    }

    // Method to display pair
    public void display() {
        System.out.println("First: " + first + ", Second: " + second);
    }
}

public class Main {
    public static void main(String[] args) {
        // Create a Pair object with Integer and String types
        Pair<Integer, String> pair1 = new Pair<>(1, "One");
        
        // Display pair
        pair1.display();
        
        // Change values
        pair1.setFirst(2);
        pair1.setSecond("Two");
        
        // Display updated pair
        pair1.display();

        // Create a Pair object with Double and Character types
        Pair<Double, Character> pair2 = new Pair<>(3.14, 'A');
        
        // Display pair
        pair2.display();
        
        // Change values
        pair2.setFirst(2.71);
        pair2.setSecond('B');
        
        // Display updated pair
        pair2.display();
    }
}
